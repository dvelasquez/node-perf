---
const { providedDelay } = Astro.props;

const delayRandomly = (providedDelay?: number) => {
  const delay = providedDelay ?? [100, 200, 300, 400, 500][Math.floor(Math.random() * 5)];
  return new Promise(resolve => setTimeout(() => resolve(delay), delay));
}

const delay = await delayRandomly(providedDelay);
---

<div>Delayed for {delay}ms</div>